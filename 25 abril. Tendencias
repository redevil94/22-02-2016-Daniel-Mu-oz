#############################
 install.packages("fpp")
 library(fpp)
 indic<- read.csv("C:\\Users\\SALAC-31\\Documents\\indicadores-inegi.csv")
 part<- ts(indic[,1], start = 2005, frequency = 4)
 plot(part)
 Acf(part)
 
 ############################## MEDIA ###################################
 ####### meanf (part, h) <. Es la media
 partpro<- meanf(part, 4)
 plot(partpro)
 ##### part es la st
 ######## el numero de datos a proyectar en este caso elegi h = 3
 
 ############################# INGENUO #################################
 partnai<- naive(part, 4)
 plot(partnai)
 rwf(part, 3) # Alternativo
 
 ############################ INGENUO ESTACIONAL #######################
 partsnai<- snaive(part, 4)
 partsnai
 plot(partsnai)
 help("snaive")
 
 ########################### METODO DE LA DERIVA #######################
 partder<- rwf(part, 4, drift = T)
 partder
 plot(partder)
 #rwf(st, h, drift=TRUE)
 
 
 ######################## EJEMPLOS 
 part1<- window(part, start=2005, end=2010)
 partaj1<- meanf(part1, h=4)
 partaj2<- naive(part1, h=4)
 partaj3<- snaive(part1, h=4)
 
 plot(part, plot.conf=FALSE, xlim=c(2005,2011),
      main="ProyecciÃ³n tasas de desempleo trimestre")
 lines(partaj1$mean, col=2)
 lines(partaj2$mean, col=3)
 lines(partaj3$mean, col=4)
 legend("topleft", lty = 1, col = c(1,2,3,4), 
        legend = c("metodo media", "metodo ingenuo", "metodo ingenuo estacional"))
 
